@model IEnumerable<SimSkills>

@if (Model.Count() == 0)
{
    <div class="center verCenter">
        <p>This Sim doesn't have ability points for any skill :(</p>
    </div>
}
else
{
    <table class="table table-striped table-bordered table-sm">
        <tr>
            
            <th>Skill</th>
            <th>Level</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Skill.Name</td>
                <td>@item.Points</td>
            </tr>
        }
    </table>
}